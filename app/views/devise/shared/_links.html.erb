
<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= button_to "Sign in with google account", omniauth_authorize_path(resource_name, provider),
      data: { turbo: false }, 
      class: " px-[20px] py-[10px] rounded-lg text-black shadow" %><br />
  <% end %>
<% end %>
